local function BOQUL_fake_script() -- TextButton_6.Script 
	local script = Instance.new('Script', TextButton_6)

	script.Parent.MouseButton1Click:Connect(function()
		local p = game:GetService("Players")
		local function r(c)
			task.wait(1) if not c then return end
			for _,v in pairs(c:GetChildren()) do 
				if v:IsA("Accessory") or v:IsA("Shirt") or v:IsA("Pants") or v:IsA("CharacterAppearance") or v:IsA("BodyColors") then v:Destroy() end
			end
			local f = c:FindFirstChild("Head") and c.Head:FindFirstChildOfClass("Decal") if f then f.Texture = "rbxassetid://72672223439434" end
			local s = Instance.new("Shirt",c) s.ShirtTemplate = "http://www.roblox.com/asset/?id=73336831221889"
		end
		for _,v in pairs(p:GetPlayers()) do if v.Character then r(v.Character) end v.CharacterAdded:Connect(r) end
		p.PlayerAdded:Connect(function(v) v.CharacterAdded:Connect(r) end)
	end)
	
end
coroutine.wrap(BOQUL_fake_script)()
local function ZAHKRDQ_fake_script() -- TextButton_7.Script 
	local script = Instance.new('Script', TextButton_7)

	script.Parent.MouseButton1Click:Connect(function()
		local chat = game:GetService("Chat")
	
		while true do
			for _, player in pairs(game.Players:GetPlayers()) do
				chat:Chat(player.Character, "TEAM BOKEY HAHAHAHA", Enum.ChatColor.White)
			end
			task.wait(0.1)
		end
	
	end)
end
coroutine.wrap(ZAHKRDQ_fake_script)()
