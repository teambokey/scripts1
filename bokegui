-- Scripts:

local function KSLQR_fake_script() -- Frame.DragScript 
	local script = Instance.new('LocalScript', Frame)

	--Not made by me, check out this video: https://www.youtube.com/watch?v=z25nyNBG7Js&t=22s
	--Put this inside of your Frame and configure the speed if you would like.
	--Enjoy! Credits go to: https://www.youtube.com/watch?v=z25nyNBG7Js&t=22s
	
	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
	
end
coroutine.wrap(KSLQR_fake_script)()
local function DEKHBXQ_fake_script() -- TextButton.Script 
	local script = Instance.new('Script', TextButton)

		local sound = Instance.new("Sound",workspace)
		sound.Name = 'China go brr'
	    sound.SoundId = "rbxassetid://109469288008880"
		sound.Looped = true
		sound.Volume = 4
		sound.Pitch = 0.1
		sound:Play()
	end)
end
coroutine.wrap(DEKHBXQ_fake_script)()
local function JAVASB_fake_script() -- TextButton_2.Script 
	local script = Instance.new('Script', TextButton_2)

	script.Parent.MouseButton1Click:Connect(function()
		local texturelol = "rbxassetid://106391438353978"
		local s = Instance.new("Sky",game:GetService("Lighting"))
		s.SkyboxBk = texturelol
		s.SkyboxDn = texturelol
		s.SkyboxFt = texturelol
		s.SkyboxLf = texturelol
		s.SkyboxRt = texturelol
		s.SkyboxUp = texturelol
	
		local sound = Instance.new("Sound",workspace)
		sound.Name = 'Team Bokey destroyed ur game lol'
		sound.SoundId = "rbxassetid://113568851878579"
		sound.Looped = true
		sound.Volume = 4
		sound.Pitch = 0.2
		sound:Play()
	
		local basics = {Color3.new(255/255,0/255,0/255),Color3.new(255/255,85/255,0/255),Color3.new(218/255,218/255,0/255),Color3.new(0/255,190/255,0/255),Color3.new(0/255,85/255,255/255),Color3.new(0/255,0/255,127/255),Color3.new(170/255,0/255,255/255),Color3.new(0/255,204/255,204/255),Color3.new(255/255,85/255,127/255),Color3.new(0/255,0/255,0/255),Color3.new(255/255,255/255,255/255)}
		game.Lighting.FogStart = 25
		game.Lighting.FogEnd = 300
		game.Lighting.Ambient = basics[math.random(1,#basics)]
		while true do
			wait(0.5)
			game.Lighting.FogColor = basics[math.random(1,#basics)]
		end
	end)
end
coroutine.wrap(JAVASB_fake_script)()
local function RUOLDWK_fake_script() -- TextButton_4.Script 
	local script = Instance.new('Script', TextButton_4)

	-- ServerScript inside the button
	local remote = script.Parent:WaitForChild("PlaceMapDecals")
	
	local DECAL_ID = "rbxassetid://106391438353978"
	
	local faces = {
		Enum.NormalId.Front,
		Enum.NormalId.Back,
		Enum.NormalId.Top,
		Enum.NormalId.Bottom,
		Enum.NormalId.Left,
		Enum.NormalId.Right
	}
	
	remote.OnServerEvent:Connect(function(player)
		for _, part in ipairs(workspace:GetDescendants()) do
			if part:IsA("BasePart") and part.Transparency < 1 then
				for _, face in ipairs(faces) do
					local decalName = face.Name .. "_Decal"
					if not part:FindFirstChild(decalName) then
						local decal = Instance.new("Decal")
						decal.Name = decalName
						decal.Face = face
						decal.Texture = DECAL_ID
						decal.Parent = part
					end
				end
			end
		end
	end)
end
coroutine.wrap(RUOLDWK_fake_script)()
local function RDRD_fake_script() -- TextButton_4.LocalScript 
	local script = Instance.new('LocalScript', TextButton_4)

	-- LocalScript inside the button
	local remote = script.Parent:WaitForChild("PlaceMapDecals")
	
	script.Parent.MouseButton1Click:Connect(function()
		remote:FireServer()
	end)
end
coroutine.wrap(RDRD_fake_script)()
local function JBRN_fake_script() -- TextButton_5.Script 
	local script = Instance.new('Script', TextButton_5)

	-- ServerScript inside HintButton
	local Players = game:GetService("Players")
	local remote = script.Parent:WaitForChild("ShowHint")
	
	local hintShown = false
	
	local function createHint(player)
		local gui = player:FindFirstChild("PlayerGui")
		if not gui then return end
		if gui:FindFirstChild("TopHint") then return end
	
		local screenGui = Instance.new("ScreenGui")
		screenGui.Name = "TopHint"
		screenGui.ResetOnSpawn = false
		screenGui.Parent = gui
	
		local textLabel = Instance.new("TextLabel")
		textLabel.Name = "HintLabel"
		textLabel.Size = UDim2.new(1, 0, 0, 50)
		textLabel.Position = UDim2.new(0, 0, 0, 0)
		textLabel.BackgroundColor3 = Color3.new(0, 0, 0)
		textLabel.TextColor3 = Color3.new(1, 1, 1)
		textLabel.Font = Enum.Font.ArialBold
		textLabel.TextSize = 24
		textLabel.Text = "screw you"
		textLabel.BorderSizePixel = 0
		textLabel.BackgroundTransparency = 0
		textLabel.TextStrokeTransparency = 0.7
		textLabel.Parent = screenGui
	end
	
	-- Button clicked
	remote.OnServerEvent:Connect(function()
		if hintShown then return end
		hintShown = true
	
		for _, player in pairs(Players:GetPlayers()) do
			createHint(player)
		end
	end)
	
	-- For new players who join later
	Players.PlayerAdded:Connect(function(player)
		player.CharacterAdded:Wait()
		if hintShown then
			task.delay(1, function()
				createHint(player)
			end)
		end
	end)
end
coroutine.wrap(JBRN_fake_script)()
